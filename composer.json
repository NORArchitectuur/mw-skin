{
  "name": "mediawiki/nora-skin",
  "type": "mediawiki-skin",
  "description": "A modern and responsive skin for MediaWiki based on the NORA design system",
  "keywords": [
    "wiki",
    "MediaWiki",
    "skin",
    "NORA",
    "design system",
    "responsive"
  ],
  "homepage": "https://www.noraonline.nl",
  "authors": [
    {
        "name": "Youri van den Bogert",
        "role": "Developer"
    }
  ],
  "support": {
    "email": "support@archixl.nl"
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "mediawiki/mediawiki-codesniffer": "43.0.0",
    "mediawiki/minus-x": "1.1.3",
    "php-parallel-lint/php-console-highlighter": "1.0.0",
    "php-parallel-lint/php-parallel-lint": "1.4.0"
  },
  "autoload": {
    "psr-4": {
      "MediaWiki\\Skin\\NORA\\": "includes/"
    }
  }
}
